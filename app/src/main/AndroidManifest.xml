<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.primera.android.tresenraya" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DosJugadoresActivity"
            android:label="@string/title_activity_dos_jugadores" >
        </activity>
        <activity
            android:name=".UnJugadorActivity"
            android:label="@string/title_activity_un_jugador" >
        </activity>
        <activity
            android:name=".TerceraActivity"
            android:label="@string/title_activity_tercera" >
        </activity>
    </application>

</manifest>
